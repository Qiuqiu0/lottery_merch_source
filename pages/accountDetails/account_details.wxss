/* pages/accountDetails/account_details.wxss */
view{
  margin: 0 auto;
  padding: 0;
}
.details_cont{
  width: 100%;
  height: 99vh;
  background: #f8f8f8;
}
.divider_view{
  width: 100%;
  height: auto;
  text-align: right;
  background: #fff;
}
.details_cont_view{
  width: 100%;
}
.cont_title{
  width: 92%;
  padding-top: 20rpx;
  padding-bottom: 18rpx;
}
.cont_label{
  display:block;
font-weight: 500;
color: #303133;
font-size: 32rpx;

}
